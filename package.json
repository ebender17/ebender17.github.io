{
  "name": "emily-bender-portfolio",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "lint:all": "npm run lint && npm run lint:css && npm run lint:html",
    "lint": "eslint \"src/ts/**/*.ts\" --max-warnings=0",
    "lint:fix": "eslint \"src/ts/**/*.ts\" --fix",
    "lint:css": "stylelint \"src/css/**/*.css\"",
    "lint:css:fix": "stylelint \"src/css/**/*.css\" --fix",
    "lint:html": "html-validate \"src/**/*.html\"",
    "lint:html:fix": "html-validate \"src/**/*.html\" --fix"
  },
  "dependencies": {
    "@babylonjs/core": "^8.32.1",
    "@humanwhocodes/momoa": "^3.3.10",
    "@stylistic/eslint-plugin": "^5.5.0",
    "@stylistic/stylelint-plugin": "^4.0.0",
    "@types/node": "^24.8.1",
    "@typescript-eslint/eslint-plugin": "^8.46.1",
    "@typescript-eslint/parser": "^8.46.1",
    "eslint": "^9.38.0",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-jsdoc": "^61.1.8",
    "eslint-plugin-jsonc": "^2.21.0",
    "eslint-plugin-no-unsanitized": "^4.1.4",
    "eslint-plugin-sonarjs": "^3.0.5",
    "eslint-plugin-unicorn": "^61.0.2",
    "html-validate": "^10.1.2",
    "stylelint": "^16.25.0",
    "stylelint-config-standard": "^39.0.1",
    "typescript": "~5.9.3",
    "vite": "^7.1.7"
  }
}
